{
    "soupdates": {
        "mappings": {
            "update": {
                "_ttl": {
                    "enabled": true,
                    "default": "3M"
                },
                "_all": {
                    "enabled": true
                },
                "dynamic": true,
                "dynamic_templates": [{
                    "geo_point_location": {
                        "path_match": "channels.location",
                        "mapping": {
                            "store": "no",
                            "index": "not_analyzed",
                            "type": "geo_point",
                            "ignore_malformed": true
                        }
                    }
                }, {
                    "string": {
                        "path_match": "channels.*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "store": "yes",
                            "index": "not_analyzed",
                            "type": "string",
                            "date_detection": false,
                            "ignore_malformed": true
                        }
                    }
                }, {
                    "double": {
                        "path_match": "channels.*",
                        "match_mapping_type": "double",
                        "mapping": {
                            "store": "yes",
                            "index": "not_analyzed",
                            "type": "double",
                            "ignore_malformed": true
                        }
                    }
                }, {
                    "integer": {
                        "path_match": "channels.*",
                        "match_mapping_type": "integer",
                        "mapping": {
                            "store": "yes",
                            "index": "not_analyzed",
                            "type": "double",
                            "ignore_malformed": true
                        }
                    }
                }],
                "properties": {
                    "id": {
                        "type": "string",
                        "analyzer": "whitespace",
                        "index": "not_analyzed"
                    },
                    "lastUpdate": {
                        "type": "date",
                        "index": "not_analyzed"
                    },
                    "userId": {
                        "type": "string",
                        "analyzer": "whitespace",
                        "index": "not_analyzed"
                    },
                    "channels": {
                        "type": "object",
                        "dynamic": false
                    },
                    "streamId": {
                        "type": "string",
                        "analyzer": "whitespace",
                        "index": "not_analyzed"
                    }
                }
            }
        }
    },
    "subscriptions": {
        "mappings": {
            "subscription": {
                "_all": {
                    "enabled": false
                },
                "dynamic": true,
                "_source": {
                    "includes": [
                        "meta.*"
                    ]
                },
                "dynamic_templates": [{
                    "all_strings_to_avoid_collisions": {
                        "match": "*",
                        "mapping": {
                            "store": "no",
                            "index": "not_analyzed",
                            "include_in_all": false,
                            "type": "string",
                            "analyzer": "whitespace"
                        }
                    }
                }],
                "properties": {
                    "callback": {
                        "type": "string"
                    },
                    "source": {
                        "type": "string"
                    },
                    "destination": {
                        "type": "string"
                    },
                    "stream": {
                        "type": "string"
                    },
                    "customFields": {
                        "enabled": false
                    }
                }
            }
        }
    },



    "sodescriptions": {
        "settings": {
            "analysis": {
                "analyzer": {
                    "path_analyzer": {
                        "tokenizer": "path_tokenizer"
                    }
                },
                "tokenizer": {
                    "path_tokenizer": {
                        "type": "path_hierarchy",
                        "delimiter": "/"
                    }
                }
            }
        },
        "mappings": {
            "serviceobject": {
                "properties": {
                    "id": {
                        "type": "string",
                        "analyzer": "whitespace",
                        "index": "not_analyzed"
                    },
                    "userId": {
                        "type": "string",
                        "analyzer": "whitespace",
                        "index": "not_analyzed"
                    },
                    "parentId": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "path": {
                        "type": "string",
                        "analyzer": "path_analyzer"
                    },
                    "createdAt": {
                        "type": "date",
                        "index": "not_analyzed"
                    },
                    "public": {
                        "type": "boolean"
                    },
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "customFields": {
                        "type": "object"
                    }
                }
            }
        }
    }


}
